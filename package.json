{
  "name": "schema3d",
  "version": "1.0.0",
  "description": "A 3D database schema visualization tool that renders database schemas as interactive 3D objects",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shane-jacobeen/schema3d.git"
  },
  "bugs": {
    "url": "https://github.com/shane-jacobeen/schema3d/issues"
  },
  "homepage": "https://github.com/shane-jacobeen/schema3d#readme",
  "keywords": [
    "database",
    "schema",
    "visualization",
    "3d",
    "react",
    "three.js",
    "sql",
    "mermaid",
    "erd"
  ],
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "NODE_ENV=production vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "db:push": "drizzle-kit push",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.5",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/node": "^2.3.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.1.6",
    "pako": "^2.1.0",
    "postcss": "^8.5.6",
    "posthog-js": "^1.331.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.170.0",
    "vite-plugin-glsl": "^1.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "4.17.21",
    "@types/node": "^22.10.0",
    "@types/pako": "^2.0.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^23.0.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "overrides": {
    "vite": "^7.2.4",
    "picomatch": "^4.0.3",
    "micromatch": {
      "picomatch": "^4.0.3"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
